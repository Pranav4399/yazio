{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".output",
  "framework": null,
  "functions": {
    "pages/**/*.vue": {
      "runtime": "nodejs18.x"
    }
  },
  "regions": ["fra1"],
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
